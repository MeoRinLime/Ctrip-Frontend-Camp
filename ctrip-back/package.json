{
  "name": "ctrip-back",
  "version": "1.0.0",
  "description": "Ctrip Travel Diary Backend Service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "init-db": "node bin/init-db.js",
    "migrate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d src/config/migration-source.ts",
    "migration:create": "typeorm migration:create",
    "migration:generate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -d src/config/migration-source.ts",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d src/config/migration-source.ts",
    "seed": "ts-node src/database/seeds/index.ts",
    "setup": "npm run init-db && npm run migrate && npm run seed"
  },
  "keywords": [
    "express",
    "typescript",
    "mysql",
    "typeorm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.2",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
